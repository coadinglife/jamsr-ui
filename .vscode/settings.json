{
  "eslint.workingDirectories": [
    {
      "mode": "auto"
    }
  ],
  "files.exclude": {
    "**/.git": false,
    "**/.svn": false,
    "**/.hg": false,
    "**/CVS": false,
    "**/.DS_Store": false,
    "**/Thumbs.db": false,
    "**/node_modules": false,
    "**/.turbo": false
  },
  "explorerExclude.backup": {},
  "cSpell.words": [
    "adityaa",
    "Booleanish",
    "clsx",
    "commitlint",
    "jamsr",
    "jamsrui",
    "jamsrworld",
    "svgr",
    "tailwindcss",
    "tsup"
  ],
  "workbench.colorCustomizations": {
    "list.warningForeground": "#00eaffcc"
  },
  "tailwindCSS.experimental.classRegex": [
    [
      "tv\\(([^)(]*(?:\\([^)(]*(?:\\([^)(]*(?:\\([^)(]*\\)[^)(]*)*\\)[^)(]*)*\\)[^)(]*)*)\\)",
      "\"(.*?)\""
    ]
  ]
}
