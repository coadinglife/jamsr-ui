{
  "eslint.workingDirectories": [
    {
      "mode": "auto"
    }
  ],
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/node_modules": true,
    "**/.turbo": true,
    "**/dist": false,
    "**/.next": true
  },
  "explorerExclude.backup": {},
  "cSpell.words": [
    "adityaa",
    "atleast",
    "Booleanish",
    "clsx",
    "commitlint",
    "hookform",
    "jamsr",
    "jamsrui",
    "jamsrworld",
    "labelledby",
    "listbox",
    "Lowlight",
    "nextjs",
    "postpack",
    "svgr",
    "tailwindcss",
    "tanstack",
    "tiptap",
    "tsup",
    "Typeahead",
    "typecheck",
    "webp",
    "zustand"
  ],
  "workbench.colorCustomizations": {
    "list.warningForeground": "#00eaffcc"
  },
  "tailwindCSS.experimental.classRegex": [
    [
      "tv\\(([^)(]*(?:\\([^)(]*(?:\\([^)(]*(?:\\([^)(]*\\)[^)(]*)*\\)[^)(]*)*\\)[^)(]*)*)\\)",
      "\"(.*?)\""
    ],
    ["Classes \\=([^;]*);", "'([^']*)'"],
    ["Classes \\=([^;]*);", "\"([^\"]*)\""],
    ["Classes \\=([^;]*);", "\\`([^\\`]*)\\`"]
  ],
  "tailwindCSS.classAttributes": [
    "class",
    "className",
    "ngClass",
    ".*Classes.*"
  ],
  "files.autoSave": "onFocusChange"
}
