{
  "eslint.workingDirectories": [
    {
      "mode": "auto"
    }
  ],
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/node_modules": true
  },
  "explorerExclude.backup": {},
  "cSpell.words": [
    "Booleanish",
    "clsx",
    "commitlint",
    "jamsr",
    "jamsrui",
    "jamsrworld",
    "svgr",
    "tailwindcss"
  ],
  "workbench.colorCustomizations": {
    "list.warningForeground": "#00eaffcc"
  },
  "tailwindCSS.experimental.classRegex": [
    [
      "tv\\(([^)(]*(?:\\([^)(]*(?:\\([^)(]*(?:\\([^)(]*\\)[^)(]*)*\\)[^)(]*)*\\)[^)(]*)*)\\)",
      "\"(.*?)\""
    ]
  ]
}
